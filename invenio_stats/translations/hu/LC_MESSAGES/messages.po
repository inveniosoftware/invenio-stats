#, python-format
msgid "Unknown statistic '%(query_name)s'"
msgstr "Ismeretlen statisztika '%(query_name)s'"

#: invenio_stats/views.py:65
#, python-brace-format
msgid ""
"Invalid Input. It should be of the form { STATISTIC_NAME: { \"stat\": "
"STAT_TYPE, \"params\": STAT_PARAMS }}"
msgstr ""
"Érvénytelen bemenet. A bemenetnek a következő formájúnak kell lennie { "
"STATISTIC_NAME: { „stat”: STAT_TYPE, „params”: STAT_PARAMS }}"

#: invenio_stats/views.py:81
#, python-format
msgid ""
"You do not have a permission to query the statistic \"%(stat)s\" with those "
"parameters"
msgstr ""
"Nincs jogosultsága a \"%(stat)s\" statisztika lekérdezésére ezekkel a "
"paraméterekkel"
