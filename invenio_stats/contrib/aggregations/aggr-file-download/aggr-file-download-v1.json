{
  "template": "stats-file-download-*",
  "settings": {
    "index": {
      "refresh_interval": "1m"
    }
  },
  "mappings": {
    "_default_": {
      "dynamic_templates": [
        {
          "date_fields": {
            "match_mapping_type": "date",
            "mapping": {
              "type": "date",
              "format": "date_optional_time"
            }
          }
        }
      ]
    },
    "file-download-aggregation": {
      "_source": {
        "enabled": true
      },
      "_all": {
        "enabled": false
      },
      "date_detection": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "date_optional_time"
        },
        "count": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "file_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "file_key": {
          "type": "string",
          "index": "not_analyzed"
        },
        "bucket_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "collection": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    },
    "file-download-bookmark": {
      "date_detection": false,
      "properties": {
        "date": {
          "type": "date",
          "format": "date_optional_time"
        }
      }
    }
  },
  "aliases": {
    "stats-file-download": {}
  }
}
